{
  "name": "graphite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:ui\" \"npm run dev:host\"",
    "dev:ui": "vite",
    "dev:host": "cd host ; cargo run",
    "build:host": "cd host ; cargo build --release",
    "build:vite": "vite build",
    "build": "npm run build:vite && npm run build:host",
    "preview": "vite preview",
    "check:svelte": "svelte-check --tsconfig ./tsconfig.json",
    "check:rust": "cd host ; cargo check",
    "check": "npm run check:rust && npm run check:svelte"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tsconfig/svelte": "^5.0.5",
    "@types/node": "^24.6.0",
    "svelte": "^5.39.6",
    "svelte-check": "^4.3.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.7"
  },
  "dependencies": {
    "concurrently": "^9.2.1",
    "phosphor-svelte": "^3.0.1"
  }
}
